                                            <fo:list-item margin-bottom="0.2em"><fo:list-item-label end-indent="label-end()"><fo:block text-align="center" font-weight="bold"><fo:inline font-family="Times New Roman, Segoe UI Symbol">◦</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">ediarum.REGISTER.edit
                                                  <fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" margin-bottom="0.5em" margin-left="0" margin-right="0" margin-top="0.5em">
                                                  <fo:list-item margin-bottom="0.2em"><fo:list-item-label end-indent="label-end()"><fo:block text-align="center" font-weight="bold"><fo:inline font-family="Times New Roman, Segoe UI Symbol">■</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>css</fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item margin-bottom="0.2em"><fo:list-item-label end-indent="label-end()"><fo:block text-align="center" font-weight="bold"><fo:inline font-family="Times New Roman, Segoe UI Symbol">■</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>resources</fo:block></fo:list-item-body></fo:list-item>
